		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.5.5",
		"acorn": "^6.2.1",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"babel-preset-react-app": "^9.0.0",
		"bson": "^4.0.2",
