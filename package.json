    "go",
    "machine-learning",
    "nodejs"
  ],
  "author": "Andrew Smith",
  "license": "ISC",
  "homepage": "https://bitbucket.org/boosmith/react-node-go-machine-learning#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "opn": "^5.5.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
