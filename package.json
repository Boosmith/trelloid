{
  "name": "react-node-go-machine-learning",
  "version": "0.1.0",
  "description": "Application to upload data for machine learning",
  "main": "index.js",
  "scripts": {
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node tools/srcServer.js",
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- -watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/boosmith/react-node-go-machine-learning.git"
  },
  "keywords": [
    "react",
    "go",
    "machine-learning",
    "nodejs"
  ],
  "author": "Andrew Smith",
  "license": "ISC",
  "homepage": "https://bitbucket.org/boosmith/react-node-go-machine-learning#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "acorn": "^6.0.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "colors": "^1.3.3",
    "css-loader": "^2.1.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "expect": "^24.1.0",
    "express": "^4.16.4",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
